{
  // 需求分支创建设置（基于最新 release_YYYYMMDD 分支创建）
  "demandBranch": {
    "prefixes": ["feature", "fix"],
    "releasePrefix": "release",
    "deepseekApiKey": "",
    "deepseekBaseUrl": "https://api.deepseek.com/v1",
    "deepseekModel": "deepseek-chat"
  },
  // 发布到测试环境按钮配置（先合并到 targetBranch，再触发 Jenkins）
  "deployToTest": {
    "targetBranch": "pre-test",
    "jenkins": {
      "url": "https://jenkins.example.com",
      "job": "team/release/deploy-test",
      "token": "",
      "user": "",
      "apiToken": "",
      "crumb": true,
      "parameters": {
        "BRANCH": "${currentBranch}",
        "COMMIT": "${headCommit}",
        "ENV": "${deployEnv}"
      }
    }
  },
  "profiles": [
    {
      "id": "merge-main",
      "sourceBranch": "",
      "targetBranch": "main",
      "strategy": "default",
      "pushAfterMerge": true,
      "pushRemote": "origin",
      "jenkins": {
        "enabled": false,
        "url": "https://jenkins.example.com",
        "job": "folder/jobName",
        "token": "",
        "user": "",
        "apiToken": "",
        "crumb": true,
        "parameters": {
          "SOURCE_BRANCH": "${sourceBranch}",
          "TARGET_BRANCH": "${targetBranch}",
          "MERGE_COMMIT": "${mergeCommit}"
        }
      }
    },
    {
      "id": "merge-release",
      "sourceBranch": "",
      "targetBranch": "release",
      "strategy": "no-ff",
      "pushAfterMerge": true,
      "pushRemote": "origin"
    }
  ]
}
